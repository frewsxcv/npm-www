<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title><%= (typeof title !== 'undefined' ? title : 'npm') %></title>
  <link rel="canonical" href="<%= canonicalHref %>">
  <link type="text/css" rel="stylesheet" media="all"
    href="/static/css/normalize.css">
  <link type="text/css" rel="stylesheet" media="all"
    href="/stylus/index.css">
  <link type="text/css" rel="stylesheet" media="all"
    href="/static/css/sh.css">
  <link type="text/css" rel="stylesheet" media="all"
        href="/static/css/charts.css">
  <link rel="search" type="application/opensearchdescription+xml"
    href="/static/css/opensearch.xml" title="Search Packages" />
<body>
  <div id="wrap"><div id="content">
    <%- include('nav.ejs', locals) %>
    <%- include('login-aside.ejs', locals) %>

    <div id="page">
      <header>
        <form action="/search" method="get">
          <% if (typeof search !== 'undefined' && typeof search.q !== 'undefined') { %>
          <input name=q type="search" placeholder="Search Packages" value="<%= sanitize(search.q) %>" tabindex="1">
          <% } else { %>
          <input name=q type="search" placeholder="Search Packages" tabindex="1">
          <% } %>
        </form>
        <h1>
          <a id="npm-logo" href="/">
            <img src="/static/img/npm.png" alt="npm">
          </a>
        </h1>
      </header>


